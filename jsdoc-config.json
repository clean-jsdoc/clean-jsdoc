{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./demo/src", "./README.md"]
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "idInHeadings": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "demo/output",
        "recurse": true,
        "template": "./",
        "tutorials": "./demo/src/tutorials",
        "theme_opts": {
            "base_url": "https://clean-jsdoc.github.io/clean-jsdoc/",
            "theme": "dynamic",
            "langNames": false,
            "moduleNames": true,
            "project": {
                "title": "clean-jsdoc",
                "version": "5.1.1",
                "repo": "https://github.com/clean-jsdoc/clean-jsdoc"
            },
            "sort": false,
            "sections": [
                "modules",
                "classes",
                "namespaces",
                "events",
                "global",
                "tutorials"
            ],
            "menu": [
                {
                    "title": "Blog",
                    "link": "https://rdipardo.bitbucket.io"
                },
                {
                    "title": "Other Projects",
                    "link": "https://bitbucket.org/rdipardo",
                    "target": "_blank"
                }
            ],
            "meta": [
                {
                    "name": "author",
                    "content": "Robert Di Pardo"
                },
                {
                    "name": "description",
                    "content": "Fork of clean-jsdoc-theme with dynamic theme switching and some backported features"
                },
                {
                    "name": "attribution",
                    "content": "Favicon made by https://www.freepik.com from https://www.flaticon.com"
                }
            ],
            "asset_paths" : [ "img", "../does/not/exist" ],
            "remote_assets": [
                {
                    "href": "https://raw.githubusercontent.com/clean-jsdoc/clean-jsdoc/c66f2cb8e2592b7a0185bf2e6160bcdd698063dd/img/html-5.png",
                    "type": "image/x-icon"
                }
            ],
            "overlay_scrollbar": {
                "options": {
                    "autoUpdate": false
                }
            },
            "codepen": {
                "options": {
                    "editors": "001",
                    "js_pre_processor": "babel"
                }
            }
        }
    }
}
