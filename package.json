{
    "name": "clean-jsdoc",
    "version": "2.3.0",
    "description": "A style-conscious fork of clean-jsdoc-theme",
    "main": "publish.js",
    "scripts": {
        "lint:js": "eslint --fix *.js static/scripts tests/specs",
        "lint:style": "stylelint static/styles/",
        "lint": "concurrently --prefix \"{time}-{pid}\" \"npm run lint:js\" \"npm run lint:style\"",
        "test": "npm run lint && ./demo/copy && node_modules/.bin/jsdoc -c jsdoc-config.json --verbose",
        "test:win": "npm run lint && demo\\copy && node_modules\\.bin\\jsdoc -c jsdoc-config.json --verbose",
        "test:ui": "wdio run ./tests/wdio.conf.js",
        "test:safari": "wdio run ./tests/wdio.safari.conf.js",
        "test:mobile": "wdio run ./tests/wdio.mobile.conf.js",
        "test:ios": "wdio run ./tests/wdio.ios.conf.js",
        "serve": "node tests/server.js",
        "serve:safari": "node tests/server.js http://bs-local.com",
        "bs": "concurrently -k -s first \"npm:serve\" \"npm:test:ui\"",
        "bs:safari": "SAFARI=1 concurrently -k -s first \"npm:serve:safari\" \"npm:test:safari\"",
        "bs:android": "concurrently -k -s first \"npm:serve\" \"npm:test:mobile\"",
        "bs:ios": "IOS=1 concurrently -k -s first \"npm:serve:safari\" \"npm:test:ios\"",
        "fetch:issue": "git checkout e71940a -- @default-tag-issue/ && npm install jsdoc --no-save",
        "browse": "npm test && xdg-open demo/output/index.html 2> /dev/null || exit 0",
        "browse:issue": "xdg-open @default-tag-issue/doc/index.html 2> /dev/null || exit 0",
        "doc:issue": "npm run fetch:issue && node node_modules/jsdoc/jsdoc.js @default-tag-issue/src/css.js -c @default-tag-issue/.jsdoc.json && npm run browse:issue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rdipardo/clean-jsdoc-theme.git"
    },
    "author": "Robert Di Pardo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rdipardo/clean-jsdoc-theme/issues"
    },
    "homepage": "https://heredocs.com/clean-jsdoc-theme",
    "peerDependencies": {
        "jsdoc": "3"
    },
    "keywords": [
        "jsdoc",
        "template",
        "jsdoc-template",
        "javascript documentation",
        "customizable jsdoc theme / template"
    ],
    "devDependencies": {
        "@jsdoc/eslint-config": "^1.1.4",
        "@wdio/cli": "^6.10.11",
        "@wdio/jasmine-framework": "^6.10.11",
        "@wdio/local-runner": "^6.10.13",
        "browserstack-local": "^1.4.8",
        "concurrently": "^5.3.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.17.0",
        "express": "^4.17.1",
        "stylelint": "^13.8.0",
        "stylelint-config-sass-guidelines": "^7.1.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "babel-preset-minify": "^0.5.1",
        "fs-extra": "^9.0.1",
        "glob": "^7.1.6",
        "minify": "^6.0.1",
        "overlayscrollbars": "^1.13.1"
    }
}
