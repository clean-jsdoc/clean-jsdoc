<!DOCTYPE html>
<?js
  var themeName = (env.opts && env.opts.theme_opts
                     && env.opts.theme_opts.theme)
                     || "light"
  var hideLangNames = env.opts && env.opts.theme_opts
                        && env.opts.theme_opts.langNames !== undefined
                        && !env.opts.theme_opts.langNames
  var noSearch = env.opts && env.opts.theme_opts
                   && env.opts.theme_opts.search !== undefined
                   && !env.opts.theme_opts.search
  var wantDate = env.conf.templates
                   && env.conf.templates.default
                   && env.conf.templates.default.includeDate !== false

  if (wantDate) {
    Date.prototype.toLongDate =
      function (locale) {
        return new Date().toLocaleDateString(locale || 'en-US', {
            year: 'numeric',
            month: 'long',
            day: 'numeric'
        });
      }
  }

  function toAssetLink(src) {
    const [ext] = (src.href || '').split('.').reverse()
    if (ext.toLowerCase() == 'css') {
       src.type = 'text/css'
       src.rel = 'stylesheet'
    } else if (!src.type && !src.rel &&
        ['jpg', 'jpeg', 'png', 'gif', 'bmp', 'ico'].includes(ext.toLowerCase())) {
       src.type = 'image/x-icon'
       src.rel = 'shortcut icon'
    }
    return src
  }
?>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <!-- Adding meta -->
    <?js if(this.meta !== undefined) { ?>
      <?js this.meta.forEach(function(src){ ?>
        <meta <?js=Object.keys(src).map(key => `${key}="${src[key]}"`).join(" ") ?>>
      <?js }) ?>
    <?js } ?>
    <title>
      <?js if (this.project !== undefined && this.project.title !== undefined) { ?>
      <?js= this.project.title ?> - <?js= title ?>
      <?js } else { ?>
      <?js= title ?>
      <?js } ?>
    </title>
    <script>
        function replaceIcon() {
            var metaField = document.querySelector('.gh-meta')
            var iconField = document.querySelector('.gh-meta p ~ p')
            var icon = document.querySelector('.fa.fa-github')
            icon.style.fontSize = 'initial'
            icon.style.lineHeight = '0'
            icon.style.fontWeight = 500
            icon.innerText = 'View Source'
            iconField.style.margin = '0'
            metaField.style.flexDirection = 'column'
        }
    </script>
    <!--[if lt IE 9]>
      <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <link type="text/css" rel="stylesheet" href="styles/third-party/ionicons.min.css">
    <link type="text/css" rel="stylesheet" href="styles/third-party/prettify-tomorrow.css">
    <link type="text/css" rel="stylesheet" href="styles/jsdoc-default.css">
    <link type="text/css" rel="stylesheet" href="styles/clean-jsdoc-theme-base.css">
    <link type="text/css" rel="stylesheet" href="styles/<?js= this.theme ?>">
    <link type="text/css" rel="stylesheet" onerror="replaceIcon()"
        href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Adding assets -->
    <?js if(this.externalAssets !== undefined) { ?>
      <?js this.externalAssets.forEach(src => { ?>
        <link <?js=Object.keys(src).map(key => `${key}="${toAssetLink(src)[key]}"`).join(" ") ?>>
      <?js }) ?>
    <?js } ?>
    <!-- Adding style-->
    <?js if(this.includeCss !== undefined) { ?>
      <?js this.includeCss.forEach(function(source){ ?>
        <link type="text/css" rel="stylesheet" href="<?js= source ?>">
      <?js }) ?>
    <?js } ?>
    <!-- Dynamic style tweaks -->
    <?js if (noSearch) { ?>
      <style>
        nav > h2 {
            padding: 0;
        }
      </style>
    <?js } ?>
    <?js if (hideLangNames) { ?>
      <style>
        .code-lang-name  {
            display: none;
        }
        .readme .prettyprint {
           padding-bottom: 0;
        }
        .readme code {
            margin: 0;
            padding-top: 0;
        }
      </style>
    <?js } ?>
    <style>
      <?js
        var isTitleSet = env.opts && env.opts.theme_opts && env.opts.theme_opts.title
        var homeTitle = isTitleSet ? env.opts.theme_opts.title.trim() : title
        var headRoom = Math.ceil(homeTitle.length * 2.5)
      ?>
      .sidebar-list-div {
          margin-top: <?js= headRoom?>px;
      }
    </style>
    <!-- Adding external script-->
    <?js if(this.dynamicScriptSrc !== undefined) { ?>
      <?js this.dynamicScriptSrc.forEach(function(src){ ?>
        <script <?js=Object.keys(src).map(key => `${key}="${src[key]}"`).join(" ") ?> ></script>
      <?js }) ?>
    <?js } ?>
    <!-- Adding scripts-->
    <?js if(this.includeScript !== undefined) { ?>
        <?js this.includeScript.forEach(function(source){ ?>
        <script src="<?js= source ?>"></script>
        <?js }) ?>
    <?js } ?>
    <!-- Adding overlay -->
    <?js if(this.overlayScrollbar !== undefined) { ?>
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/overlayscrollbars/1.13.0/css/OverlayScrollbars.min.css" integrity="sha512-pYQcc5kgavar0ah58/O8hw/6Tbo3mWlmQTmvoi1i96cBz7jQYS9as5J+Nfy32rAHY6CgR9ExwnFMcBdGVcKM7g==" crossorigin="anonymous" />
      <script src="https://cdnjs.cloudflare.com/ajax/libs/overlayscrollbars/1.13.0/js/OverlayScrollbars.min.js" integrity="sha512-5R3ngaUdvyhXkQkIqTf/k+Noq3phjmrqlUQyQYbgfI34Mzcx7vLIIYTy/K1VMHkL33T709kfh5y6R9Xy/Cbt7Q==" crossorigin="anonymous"></script>
    <?js } ?>
</head>
<body>
<?js= this.partial('icons.tmpl') ?>
<input type="checkbox" id="nav-trigger" class="nav-trigger" />
<label for="nav-trigger" class="navicon-button x">
  <span class="navicon"></span>
</label>
<label for="nav-trigger" class="overlay"></label>

<nav>
    <?js if (this.project !== undefined) { ?>
    <div class="gh-meta">
    <p><b>Version</b> <?js= this.project.version ?></p>
    <p>
      <a href="<?js= this.project.repo ?>" title="View project on Github"><span class="fa fa-github" aria-hidden="true"></span></a>
    </p>
    </div>
    <?js } ?>
    <?js= this.nav ?>
</nav>

<div id="main">
    <?js if (title.toLowerCase() !== 'home') { ?>
    <h1 class="page-title"><?js= title ?></h1>
    <?js } ?>
    <?js= content ?>
</div>

<footer id="footer">
  <?js if (this.footer) { ?>
  <?js= this.footer ?>
  <?js } else { ?>
  Generated by <a href="https://github.com/jsdoc3/jsdoc">JSDoc <?js= env.version.number ?></a><?js if(wantDate) { ?> on <?js= (new Date().toLongDate()) ?><?js } ?> using the <a href="https://github.com/rdipardo/clean-jsdoc-theme">clean-jsdoc</a> <?js= themeName ?> theme.
  <?js } ?>
</footer>

<script src="scripts/third-party/prettify.js"></script>
<script src="scripts/third-party/lang-css.js"></script>
<script src="scripts/third-party/fuse.js"></script>
<script>prettyPrint();</script>
<script src="scripts/linenumber.js"></script>
<script src="scripts/fix-code-block.js"></script>
<script src="scripts/search.js"></script>
<script src="scripts/misc.js"></script>
<script>
if (document.getElementById('search-box')) {
    var list = [<?js= this.searchList ?>];
    setupSearch(list)
}
</script>

<?js if(this.overlayScrollbar !== undefined ) { ?>
  <script type="text/javascript">
    var option = JSON.parse('<?js=this.overlayScrollbar ?>')
    console.log(option)
    document.addEventListener("DOMContentLoaded", function() {
    OverlayScrollbars(document.querySelectorAll('body'), option.option || {});
  });
  </script>
<?js } ?>

 <?js if (this.dynamicScript !== undefined ) { ?>
  <script>
    <?js= this.dynamicScript ?>
  </script>
<?js } ?>
</body>
</html>
