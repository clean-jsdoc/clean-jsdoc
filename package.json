{
    "name": "clean-jsdoc",
    "version": "2.2.14.02",
    "description": "A stylish fork of clean-jsdoc-theme",
    "main": "publish.js",
    "scripts": {
        "lint:js": "eslint --fix static/scripts",
        "lint:style": "stylelint static/styles/",
        "lint:win": "npm run lint:js && npm run lint:style",
        "lint": "concurrently --prefix '{time}-{pid}' 'npm run lint:js' 'npm run lint:style'",
        "test": "npm run lint && ./demo/copy && node_modules/.bin/jsdoc -c jsdoc-config.json",
        "test:win": "npm run lint:win && demo\\copy && node_modules\\.bin\\jsdoc -c jsdoc-config.json",
        "browse": "npm test && xdg-open demo/output/index.html 2> /dev/null || exit 0",
        "fetch:issue": "git checkout e71940a -- @default-tag-issue/ && npm install jsdoc --no-save",
        "browse:issue": "xdg-open @default-tag-issue/doc/index.html 2> /dev/null || exit 0",
        "doc:issue": "npm run fetch:issue && node node_modules/jsdoc/jsdoc.js @default-tag-issue/src/css.js -c @default-tag-issue/.jsdoc.json && npm run browse:issue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rdipardo/clean-jsdoc-theme.git"
    },
    "author": "Robert Di Pardo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rdipardo/clean-jsdoc-theme/issues"
    },
    "homepage": "https://heredocs.com/demos/js/clean-jsdoc",
    "peerDependencies": {
        "jsdoc": "3"
    },
    "keywords": [
        "jsdoc",
        "template",
        "jsdoc-template",
        "javascript documentation",
        "customizable jsdoc theme / template"
    ],
    "devDependencies": {
        "@jsdoc/eslint-config": "^1.1.4",
        "concurrently": "^5.3.0",
        "eslint": "^7.11.0",
        "prettier": "^2.1.2",
        "stylelint": "^13.7.2",
        "stylelint-config-sass-guidelines": "^7.1.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0"
    }
}
