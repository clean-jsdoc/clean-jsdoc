                   && env.opts.theme_opts.search !== undefined
                   && !env.opts.theme_opts.search
  var wantDate = env.conf.templates
                   && env.conf.templates.default
                   && env.conf.templates.default.includeDate !== false

  if (wantDate) {
    Date.prototype.toLongDate =
