{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "staticFiles": {
                "include": [
                  "@default-tag-issue"
                ],
                "includePattern": ".+\\.(js(doc|x)|css|png)?$",
                "excludePattern": ".+\\.(md*|html|tmpl|js(on|x|))$"
            }
        }
    },
    "opts": {
        "template": ".",
        "theme_opts": {
            "theme": "dark",
            "search": false,
            "add_style_path": ["img-border.css"],
            "footer": "<h6><a href=\"https://github.com/rdipardo/clean-jsdoc-theme/tree/master/%40default-tag-issue\">View source</a></h6>"
        },
        "encoding": "utf8",
        "destination": "@default-tag-issue/doc/",
        "recurse": false,
        "readme": "@default-tag-issue/README.md"
    }
}
